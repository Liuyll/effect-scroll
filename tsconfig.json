{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "jsx": "react",
      "module": "commonjs",
      "noImplicitAny": false,
      "outDir": "./build/",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "target": "es5",
      "baseUrl": "./"
    },
    "include": [
      "./src/**/*"
    ],
    "exclude": ["node_modules","dist"]
  }